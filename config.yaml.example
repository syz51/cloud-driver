# Cloud Driver Server Configuration
# Copy this file to config.yml and modify as needed

server:
  host: "0.0.0.0"  # Server bind address (0.0.0.0 for all interfaces)
  port: 8081       # Server port


# You can also set these via environment variables:
# CLOUD_DRIVER_SERVER_HOST=0.0.0.0
# CLOUD_DRIVER_SERVER_PORT=8080
